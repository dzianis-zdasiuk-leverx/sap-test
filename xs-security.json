{
  "xsappname": "demo",
  "tenant-mode": "dedicated",
  "oauth2-configuration": {
    "redirect-uris": [
      "https://*.us10-001.hana.ondemand.com/login/callback"
    ]
  },
  "scopes": [
    {
      "name": "$XSAPPNAME.Display",
      "description": "Display content"
    }
  ],
  "role-templates": [
    {
      "name": "Viewer",
      "description": "View content",
      "scope-references": [
        "$XSAPPNAME.Display"
      ]
    }
  ]
}